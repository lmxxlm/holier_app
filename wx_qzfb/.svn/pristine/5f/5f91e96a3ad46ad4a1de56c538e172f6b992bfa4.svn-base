package com.danrong.wx.qzfb.module.department;

import java.io.Serializable;
import java.util.List;

import com.danrong.wx.qzfb.module.doctor.Doctor;

public class SecondDepartment extends Department implements Serializable {

  private static final long serialVersionUID = 7501688275037522076L;

  private List<Doctor> doctorList;
  private String departmentIntro;

  public List<Doctor> getDoctorList() {
    return doctorList;
  }

  public void setDoctorList(List<Doctor> doctorList) {
    this.doctorList = doctorList;
  }

  public String getDepartmentIntro() {
    return departmentIntro;
  }

  public void setDepartmentIntro(String departmentIntro) {
    this.departmentIntro = departmentIntro;
  }

}
