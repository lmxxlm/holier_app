@charset "utf-8";
/* CSS Document */
*{margin:0;padding:0;}
body{background:#EEF1F3;font-size: 16px;}
ul,li{list-style-type:none;
margin:0;padding:0;}
/*头部*/
div.head{
	display:inline-block;
	width:100%;
	text-align:center;
	position:fixed;
	top:0px;
	height:3em;
	line-height:3em;
	font-size:1.3em;
	background:#fff;
	color:#551A8B;
	/*color:red;*/
	letter-spacing:0.3em;
	}
	
/*列表*/
div.list{
	width:100%;
	margin-top:4.5em;
	background:#fff;
	padding:1em 0;
	}
 li{
  margin-left:1em;
   height:2em;
   line-height:2em;
   letter-spacing:0.2em;
   
 }
ul li a{
	color:#551A8B;
	text-decoration:none;
	
	display:block;
	white-space: nowrap; 
	text-overflow: ellipsis; 
    -o-text-overflow: ellipsis; 
	-webkit-text-overflow: ellipsis; 
	overflow: hidden; 
	} 