@charset "utf-8";
/* CSS Document */
body{background:#F0F0F0;color:#B3B3B3;}
p{margin:0;padding:0;}
.main{
	width:100%;
	margin:0 auto;
	display:block;
	
	}

.main section{
	margin:0 auto;
	width:85%;
	padding:4%;
	background:#FFF;
	border:1px solid #BCBCBC;
	border-radius:1em;
	margin-top:20%;
	}
.main .part .p1{
	color:#1E1E1E;
	font-size:1.2em;
	}
.main .part .p2{
	margin-top:3%;
	font-size:0.8em;
	}
.main .part .p3{
	font-size:0.9em;
	margin:4% 0;
	}
.main .part .p4{
	font-size:0.9em;
	
	}
	
.main .part{
	border-bottom:1px solid #B3B3B3;
	padding-bottom:10%;
	}
	
.main .span{
	margin-top:2%;
	color:#060606;
	font-size:0.9em;
	}
	


.main .span .span2{float:right;}